<h1>Listing Students</h1>
<table>
    <thead>
        <tr>
            <th>First name</th>
            <th>last name</th>
            <th>Email</th>

        </tr>
    </thead>
    <tbody>
        <% @students.each do |student| %>
        <tr>
            <td><%= student.first_name %></td>
            <td><%= student.last_name %></td>
            <td><%= student.email %></td>

        </tr>
        <% end %>
    </tbody>
</table>